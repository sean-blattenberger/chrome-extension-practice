{
  "manifest_version": 2,
  "name": "gifTabs",
  "description": "Generates meme or gif based on your mood",
  "version": "1",
  "author": "Sean Blattenberger",
  "browser_action": {
   "default_icon": "icon.png",
   "default_title": "My chrome app"
 },
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "permissions": ["activeTab"]
}